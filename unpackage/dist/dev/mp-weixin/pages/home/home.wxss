.create_vote_list {
  padding: 10px;
}
.create_vote_list .vote_list .vote_item {
  border-radius: 5px;
  background-color: white;
  display: flex;
  width: 100%;
  flex-direction: row;
  margin-bottom: 15px;
}
.create_vote_list .vote_list .vote_item .vote_item_pic {
  background: url("data:image/png;base64,") no-repeat center;
  width: 3rem;
  height: 3rem;
  margin-right: 0.425rem;
  background-size: cover;
  margin: 30rpx;
}
.create_vote_list .vote_list .vote_item .vote_item_word {
  background: url("data:image/png;base64,") no-repeat center;
  width: 3rem;
  height: 3rem;
  margin-right: 0.425rem;
  background-size: cover;
  margin: 30rpx;
}
.create_vote_list .vote_list .vote_item .vote_item_body {
  height: auto;
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
  overflow: hidden;
}
.create_vote_list .vote_list .vote_item .vote_item_body .vote_item_title {
  font-size: 1.2rem;
  overflow: hidden;
  width: 100%;
  font-weight: bolder;
  padding-top: 10px;
}
.create_vote_list .vote_list .vote_item .vote_item_body .vote_item_status_progressive {
  width: 100%;
  line-height: 1.1375rem;
  font-size: 0.9rem;
  padding-bottom: 10px;
  color: blue;
}
.create_vote_list .vote_list .vote_item .vote_item_body .vote_item_status_over {
  width: 100%;
  line-height: 1.1375rem;
  font-size: 0.9rem;
  padding-bottom: 10px;
  color: gray;
}